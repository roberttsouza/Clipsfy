{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "source": "/(.*)",
      "destination": "/" ,
      "methods": ["GET"]
    },
    {
      "src": "/process",
      "dest": "/process",
      "methods": ["POST"]
    }
  ],
  "public": false
}
