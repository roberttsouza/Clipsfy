{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "build.sh",
      "use": "@vercel/shell"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET"]
    },
    {
      "src": "/process",
      "dest": "/process",
      "methods": ["POST"]
    }
  ]
}
